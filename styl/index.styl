body
  color: #333;
  font-family: arial, helvetica;
  background: #cde6fa;

h1
  font-size: 16px;
  padding: 10px 0;
  margin: 0;

h2
  font-size: 14px;

a
  font-size: 12px;
  color: #333;

.finder
  padding: 0 20px 20px 20px;

ul
  padding: 0;
  list-style-type: none;
  border: 1px solid #a8d4f7;
  border-radius: 3px;

li
  line-height: 14px;
  padding: 3px 2px;

  &:nth-child(odd)
    background: #cde6fa;

  &:nth-child(even)
    background: #a8d4f7;
